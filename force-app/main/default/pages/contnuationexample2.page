<apex:page controller="continuationexample2" showChat="false" showHeader="false">
   <apex:form >
      <!-- Invokes the action method when the user clicks this button. -->
      <apex:commandButton action="{!startRequest}" 
              value="Start Request" reRender="panel"/> 
   </apex:form>

    <apex:outputPanel id="panel">
   <!-- This output text component displays the callout response body. -->
   <apex:outputText value="{!result1}" />
    
    <apex:outputText value="{!result2}" />
        
    <apex:outputText value="{!result3}" />
        
    <!--  <apex:outputText value="{!result4}" /> -->
    </apex:outputPanel> 
    
</apex:page>